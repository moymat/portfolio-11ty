{
	"name": "portfolio-11ty",
	"version": "1.0.0",
	"description": "Portfolio project with 11ty",
	"main": "index.js",
	"scripts": {
		"prestart": "run-s clean",
		"start": "run-p *:dev",
		"prebuild": "run-s clean",
		"build": "NODE_ENV=production run-s eleventy:build vite:build",
		"clean": "run-s clean:files clean:empty",
		"clean:files": "del 'src/vite/**/*.html' public",
		"clean:empty": "delete-empty src",
		"eleventy:dev": "eleventy --watch",
		"eleventy:build": "eleventy",
		"vite:dev": "vite",
		"vite:build": "vite build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/moymat/portfolio-11ty.git"
	},
	"author": "Michel Deckert",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/moymat/portfolio-11ty/issues"
	},
	"homepage": "https://github.com/moymat/portfolio-11ty#readme",
	"devDependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-img": "^1.0.0",
		"del-cli": "^4.0.1",
		"delete-empty": "^3.0.0",
		"eleventy-plugin-youtube-embed": "^1.6.5",
		"fast-glob": "^3.2.11",
		"npm-run-all": "^4.1.5"
	},
	"dependencies": {
		"canvas-image-cover": "^2.0.2",
		"eleventy-plugin-sass": "^1.2.0",
		"esbuild": "^0.14.2",
		"esbuild-sass-plugin": "^1.8.0",
		"markdown-it": "^12.3.0",
		"markdown-it-attrs": "^4.1.0",
		"tiny-glob": "^0.2.9",
		"vite": "^2.8.0"
	}
}
